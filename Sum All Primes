

function sumPrimes(num) {
var c = 0;
var total = 0;
for (i = 1; i <= num; i++) {
    for (j = 2; j <= i; j++) {
        if (i % j === 0) {
        c++;    
        }
    }
    if (c === 1) {
        console.log(i);
      total = total + i;
    }
    c = 0;
  }
   
 
  return total;
}

sumPrimes(977);
