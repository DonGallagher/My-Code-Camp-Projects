function findLongestWord(str) {
  strings=[];
  count=0;
  max=0;
  strings=str.split(" ");
  
  while (count < strings.length) {
   if (strings[count].length >= max) {
      max=strings[count].length;
         }
 
     count ++;
  }

   return max;
}

findLongestWord("The quick brown fox jumped over the lazy dog");
